generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}



model User {
  id      Int     @id @default(autoincrement())
  totalTime   String?
  createdAt   String?
  firstName   String?
  lastName   String?
  score   String?
  totalClick   Int?
  totalSkip    Int?
  picture  Picture[]
}

model Picture {
  id      Int     @id @default(autoincrement()) 
  userId   Int
  skipCounts  Json?
  timeCounts  Json?
  clickCounts Json?
  scoreCounts Json?


  user    User @relation(fields: [userId],references: [id])
}


